{
  "name": "Упорядочить тикеты",
  "description": "Упорядочивает тикеты в порядке возрастания модулей, возможна фильтрация по компонентам",
  "version": "1.1",
  "manifest_version": 3,

  "permissions": ["activeTab", "scripting"],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },

  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches":["https://bot.yandex-team.ru/*"],
            "css":["extention.css"]
    }
]
}